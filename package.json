{
  "name": "ag-grid-svelte",
  "version": "0.3.0",
  "description": "An unofficial Svelte wrapper for AG Grid",
  "type": "module",
  "devDependencies": {
    "@playwright/test": "^1.42.1",
    "@sveltejs/adapter-static": "^3.0.1",
    "@sveltejs/kit": "^2.5.3",
    "@sveltejs/package": "^2.3.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "ag-grid-community": "^30.0.0",
    "ag-grid-enterprise": "^30.0.0",
    "commit-and-tag-version": "^12.2.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "highlight.js": "^11.9.0",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.2",
    "sass": "^1.71.1",
    "svelte": "^4.2.12",
    "svelte-check": "^3.6.7",
    "svelte-preprocess": "^5.1.3",
    "svelte2tsx": "^0.7.4",
    "tslib": "^2.6.2",
    "typescript": "^5.4.2",
    "vite": "^5.1.6"
  },
  "peerDependencies": {
    "ag-grid-community": "^28 || ^29 || ^30",
    "svelte": "^4"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "package": "svelte-package",
    "preview": "vite preview",
    "test": "playwright test",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint .",
    "format": "prettier --write --plugin-search-dir=. .",
    "release": "commit-and-tag-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MichaelKim/ag-grid-svelte.git"
  },
  "keywords": [],
  "author": "Michael Kim",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MichaelKim/ag-grid-svelte/issues"
  },
  "homepage": "https://github.com/MichaelKim/ag-grid-svelte#readme",
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "svelte": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  }
