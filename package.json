{
  "name": "ag-grid-svelte",
  "version": "0.2.0",
  "description": "An unofficial Svelte wrapper for AG Grid",
  "type": "module",
  "dependencies": {
    "svelte": "^3.55.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.22.2",
    "@sveltejs/adapter-static": "^2.0.1",
    "@sveltejs/kit": "^1.10.0",
    "@sveltejs/package": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "ag-grid-community": "^28.2.1",
    "ag-grid-enterprise": "^28.2.1",
    "commit-and-tag-version": "^11.2.1",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "highlight.js": "^11.7.0",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.9.0",
    "sass": "^1.54.0",
    "svelte-check": "^3.0.4",
    "svelte-preprocess": "^5.0.1",
    "svelte2tsx": "^0.6.2",
    "tslib": "^2.3.1",
    "typescript": "^4.9.5",
    "vite": "^4.1.4"
  },
  "peerDependencies": {
    "ag-grid-community": "^28 || ^29"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "package": "svelte-package",
    "preview": "vite preview",
    "test": "playwright test",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint .",
    "format": "prettier --write --plugin-search-dir=. .",
    "release": "commit-and-tag-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MichaelKim/ag-grid-svelte.git"
  },
  "keywords": [],
  "author": "Michael Kim",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MichaelKim/ag-grid-svelte/issues"
  },
  "homepage": "https://github.com/MichaelKim/ag-grid-svelte#readme",
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "svelte": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  }
}
